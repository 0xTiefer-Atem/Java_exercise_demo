<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LogIn</servlet-name>
        <servlet-class>org.Servlet.LogIn</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LogIn</servlet-name>
        <url-pattern>/LogIn</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>org.Servlet.Register</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterDriverServlet</servlet-name>
        <servlet-class>org.Servlet.RegisterDriverServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterDriverServlet</servlet-name>
        <url-pattern>/RegisterDriverServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>DriverMess</servlet-name>
        <servlet-class>org.Servlet.DriverMess</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DriverMess</servlet-name>
        <url-pattern>/DriverMess</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EditServlet</servlet-name>
        <servlet-class>org.Servlet.EditServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditServlet</servlet-name>
        <url-pattern>/EditServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserRes</servlet-name>
        <servlet-class>org.Servlet.UserRes</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserRes</servlet-name>
        <url-pattern>/UserRes</url-pattern>
    </servlet-mapping>
</web-app>